<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<MenuBar prefHeight="20.0" prefWidth="300.0" BorderPane.alignment="CENTER" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hour.controller.MenuController">
<menus>
    <Menu fx:id="file" mnemonicParsing="false" text="文件" />
    <Menu fx:id="tools" mnemonicParsing="false" text="工具">
        <items>
            <MenuItem fx:id="openCalculator" mnemonicParsing="false" onAction="#openCalculator" text="计算器" />
            <MenuItem fx:id="openCalendar" mnemonicParsing="false" onAction="#openCalender" text="日历" />
        </items>
    </Menu>
    <Menu fx:id="person" mnemonicParsing="false" text="联系人">
        <items>
            <MenuItem fx:id="openPerson" mnemonicParsing="false" onAction="#openPerson" text="打开联系人" />
        </items></Menu>
    <Menu fx:id="imgs" mnemonicParsing="false" text="相册">
        <items>
            <MenuItem fx:id="openImg" mnemonicParsing="false" onAction="#openImg" text="打开相册" />
            <MenuItem fx:id="addImg" mnemonicParsing="false" onAction="#addImg" text="添加图片" />
        </items></Menu>
    <Menu fx:id="mv" mnemonicParsing="false" text="播放器">
        <items>
            <MenuItem id="chooseFile" fx:id="chooseFile" mnemonicParsing="false" onAction="#chooseMV" text="选择文件" />
            <MenuItem id="openMV" fx:id="openMV" mnemonicParsing="false" onAction="#toMV" text="播放视频" />
        </items></Menu>
</menus>
</MenuBar>
